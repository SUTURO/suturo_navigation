<launch>
  <arg name="run_obstacle_finder" default="true"/>
  <arg name="run_nav_fix" default="true"/>

  <include file="$(find snap_map_icp)/launch/snap_map_icp.launch" />

  <node pkg="suturo_navigation_fix" name="suturo_navigation_fix" type="nav_fix.py" output="screen" 
		if="$(eval run_nav_fix == true)"/>

  <node pkg="obstacle_finder" name="obstacle_finder" type="object_finder.py" output="screen"
		if="$(eval run_obstacle_finder == true)"/>
</launch>
